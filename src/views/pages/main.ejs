<!DOCTYPE html>

<% if (lang == 'en') { %>
  <html lang="en">
<% } else { %>
  <html lang="ar" dir="rtl" class="rtl">
<% } %>
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Meta -->
    <meta name="description" content="Monitoring of Housing Reconstruction in Awaran">
    <meta name="author" content="Cyberstate Technologies - SFD's Consultant">

    <title>HRA Portal</title>

    <!-- vendor css -->
    <link href="theme/lib/font-awesome/css/font-awesome.css" rel="stylesheet">
    <link href="theme/lib/Ionicons/css/ionicons.css" rel="stylesheet">
    <link href="theme/lib/perfect-scrollbar/css/perfect-scrollbar.css" rel="stylesheet">
    <link href="theme/lib/jquery-switchbutton/jquery.switchButton.css" rel="stylesheet">
    <link href="theme/lib/highlightjs/github.css" rel="stylesheet">
    <link href="theme/lib/datatables/jquery.dataTables.css" rel="stylesheet">
    <link href="theme/lib/select2/css/select2.min.css" rel="stylesheet">

    <!-- Bracket CSS -->
    <link rel="stylesheet" href="theme/css/bracket.css">
  </head>

  <body>
    <% include ../partials/header-main %>

    <!-- ########## START: MAIN PANEL ########## -->
    <% if (lang == 'en') { %>
      <div class="br-mainpanel" style="margin-left:5px;">
    <% } else { %>
      <div class="br-mainpanel" style="margin-right:5px;">
    <% } %>
    
      <div class="br-pageheader">
        <nav class="breadcrumb pd-0 mg-0 tx-12">
            <% if (lang == 'en') { %>
              <a class="breadcrumb-item" href="/">Home</a>
              <span class="breadcrumb-item active">Main</span>
              <% } else { %>
                <a class="breadcrumb-item" href="/">الصفحة الرئيسية</a>
              <span class="breadcrumb-item active">الصفحة الرئيسية</span>
              <% } %>
        </nav>
      </div><!-- br-pageheader -->
      <div class="br-pagetitle">
        <i class="icon icon ion-ios-home"></i>
        <div>
            <% if (lang == 'en') { %>
              <h4>HRA Monitoring Project</h4>
              <p class="mg-b-0">September 2018 - April 2020</p>
            <% } else { %>
              <h4> مراقبة المشروع HRA</h4>
              <p class="mg-b-0">سبتمبر 2018 - أبريل 2020</p>
            <% } %>
          
        </div>
      </div><!-- d-flex -->

      <div class="br-pagebody">
        <div class="br-section-wrapper">
            <div class="row">
                <div class="col-md">
                  <div class="card bd-0">
                    <div class="card-body rounded-top bd-0 bg-info tx-white">
                      <h6 class="mg-b-3">
                        <a href="/hc-dashboard" class="tx-white">
                          
                          <% if (lang == 'en') { %>
                            Construction of 8000 Houses
                          <% } else { %>
                            بناء 8000 منزل
                          <% } %>
                        </a>
                      </h6>
                      <span class="tx-12"></span>
                    </div><!-- card-body -->
                    <img class="card-img-bottom img-fluid" src="images/scope_hra.jpg" alt="Image">
                    <div class="card-footer">
                        <a href="/hc-dashboard">
                          <% if (lang == 'en') { %>
                            View Dashboard
                          <% } else { %>
                            عرض لوحة التحكم
                          <% } %>
                        </a>
                    </div><!-- card-footer -->
                    <div>
                        <h5 class="mg-b-3 tx-center">LAST UPDATED: January 27, 2020</h5>
                    </div>
                  </div><!-- card -->
                </div><!-- col -->
                <div class="col-md mg-t-20 mg-md-t-0">
                  <div class="card bd-0">
                    <div class="card-body rounded-top bd-0 bg-primary tx-white">
                      <h6 class="mg-b-3"><a href="" class="tx-white hover-white-8">
                          <% if (lang == 'en') { %>
                            Construction of Government Building
                          <% } else { %>
                            بناء مبنى حكومي
                          <% } %></a></h6>
                      <span class="tx-12 tx-white-8"></span>
                    </div><!-- card-body -->
                    <img class="card-img-bottom img-fluid" src="images/govt-building.jpg" alt="Image">
                    <div class="card-footer">
                        <a href="/">
                          <% if (lang == 'en') { %>
                            View Dashboard
                          <% } else { %>
                            عرض لوحة التحكم
                          <% } %>
                        </a>
                    </div><!-- card-footer -->
                  </div><!-- card -->
                </div><!-- col -->
                <div class="col-md mg-t-20 mg-md-t-0">
                  <div class="card bd-0">
                    <div class="card-body rounded-top bg-warning tx-white-8">
                      <h6 class="mg-b-3"><a href="" class="tx-white-8 hover-white">
                          <% if (lang == 'en') { %>
                            Livelihood
                          <% } else { %>
                            رزق - اقتصاد
                          <% } %></a></h6>
                      </a></h6>
                      <span class="tx-12"></span>
                    </div><!-- card-body -->
                    <img class="card-img-bottom img-fluid" src="images/livelihood.jpg" alt="Image">
                    <div class="card-footer">
                        <a href="/">
                          <% if (lang == 'en') { %>
                            View Dashboard
                          <% } else { %>
                            عرض لوحة التحكم
                          <% } %>
                        </a>
                    </div><!-- card-footer -->
                  </div><!-- card -->
                </div><!-- col -->
              </div>
        </div><!-- br-section-wrapper -->
      </div><!-- br-pagebody -->
      <% include ../partials/footer %>
    </div><!-- br-mainpanel -->
    <!-- ########## END: MAIN PANEL ########## -->

    <script src="theme/lib/jquery/jquery.js"></script>
    <script src="theme/js/popper.js"></script>
    <script src="theme/lib/bootstrap/js/bootstrap.js"></script>
    <script src="theme/lib/perfect-scrollbar/js/perfect-scrollbar.jquery.js"></script>
    <script src="theme/lib/moment/moment.js"></script>
    <script src="theme/lib/jquery-ui/jquery-ui.js"></script>
    <script src="theme/lib/jquery-switchbutton/jquery.switchButton.js"></script>
    <script src="theme/lib/peity/jquery.peity.js"></script>
    <script src="theme/lib/highlightjs/highlight.pack.js"></script>
    <script src="theme/lib/datatables/jquery.dataTables.js"></script>
    <script src="theme/lib/datatables-responsive/dataTables.responsive.js"></script>
    <script src="https://cdn.datatables.net/fixedheader/3.1.5/js/dataTables.fixedHeader.min.js"></script>
    <script src="theme/lib/select2/js/select2.min.js"></script>

    <script src="theme/js/bracket.js"></script>
    

    <script>

      $(function(){
        $(document).ready(function() {  

        });
      });
    </script>
  </body>
</html>
