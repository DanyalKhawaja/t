        function createpoly(path, colorIndex) {
            return new google.maps.Polygon({
                paths: path,
                strokeColor: strokeColor,
                strokeOpacity: strokeOpacity,
                strokeWeight: 1,
                fillColor: fillcolors[colorIndex],
                fillOpacity: fillOpacity,
                label: 'string'
            });
